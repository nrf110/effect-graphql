{
  "name": "@effect-graphql/web",
  "version": "0.1.0",
  "description": "Web standard handler for @effect-graphql/core - works with Cloudflare Workers, Deno, and other WASM runtimes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "@effect-graphql/core": "^0.1.0",
    "@effect/platform": "^0.94.0",
    "effect": "^3.19.0"
  },
  "devDependencies": {
    "@effect-graphql/core": "*",
    "@effect/platform": "^0.94.0",
    "effect": "^3.19.13"
  },
  "keywords": [
    "effect",
    "graphql",
    "cloudflare",
    "workers",
    "wasm",
    "web",
    "deno"
  ],
  "license": "MIT"
}
