{
  "name": "@effect-gql/core",
  "version": "0.1.0",
  "description": "Core GraphQL framework for Effect-TS - schema building, type mapping, and execution",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./builder": {
      "types": "./dist/builder/index.d.ts",
      "default": "./dist/builder/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "@effect/platform": "^0.94.0",
    "effect": "^3.19.0",
    "graphql": "^16.0.0",
    "graphql-ws": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "graphql-ws": {
      "optional": true
    }
  },
  "dependencies": {
    "dataloader": "^2.2.3",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@effect/platform": "^0.94.0",
    "effect": "^3.19.13",
    "graphql": "^16.0.0",
    "graphql-ws": "^6.0.6"
  },
  "keywords": [
    "effect",
    "graphql",
    "typescript",
    "schema",
    "functional-programming"
  ],
  "license": "MIT"
}
