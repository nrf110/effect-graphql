{
  "name": "@effect-gql/cli",
  "version": "0.1.0",
  "description": "CLI tools for Effect GraphQL development",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "effect-gql": "./dist/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "@effect-gql/core": "^0.1.0",
    "effect": "^3.19.0",
    "graphql": "^16.0.0"
  },
  "devDependencies": {
    "@effect-gql/core": "*",
    "effect": "^3.19.13",
    "graphql": "^16.0.0"
  },
  "keywords": [
    "effect",
    "graphql",
    "cli",
    "schema",
    "codegen"
  ],
  "license": "MIT"
}
